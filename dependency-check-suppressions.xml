<?xml version="1.0" encoding="UTF-8"?>
<suppressions xmlns="https://jeremylong.github.io/DependencyCheck/dependency-suppression.1.3.xsd">
    <!-- 
    Suppression file for OWASP Dependency Check false positives
    See https://jeremylong.github.io/DependencyCheck/general/suppression.html
    -->
    
    <!-- Example suppression for common false positives in Kotlin/Android projects -->
    <suppress>
        <notes><![CDATA[
        Suppress false positives for test dependencies that don't affect production
        ]]></notes>
        <cve>CVE-2016-1000027</cve>
        <gav regex="true">.*:spring-web:.*</gav>
    </suppress>
    
    <!-- Suppress vulnerabilities in build/test-only dependencies -->
    <suppress>
        <notes><![CDATA[
        Test dependencies don't affect production runtime
        ]]></notes>
        <gav regex="true">.*:junit:.*</gav>
    </suppress>
    
</suppressions>
